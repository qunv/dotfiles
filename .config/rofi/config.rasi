configuration {
  modi: "window,drun";
  drun-display-format: "{icon} {name}";
  show-icons: true;
  icon-theme: "Papirus";
  kb-remove-to-eol: "Control+p";
  kb-accept-entry: "Control+m,Return,KP_Enter";
  kb-row-up: "Up,Control+k";
  kb-row-down: "Down,Control+j";
  timeout {
      action: "kb-cancel";
      delay:  0;
  }
  filebrowser {
      directories-first: true;
      sorting-method:    "name";
  }
}

@theme "arthur"
